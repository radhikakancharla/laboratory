<h1>Duplicate Counter</h1>
<form  method ='post' action ='/math/duplicatecounter'>
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type ='text' name ='numbers' />
    <input type ='submit' name ='Count Duplicates' />
</form>

<% if @counts %>
    <ul>
        <% @counts.each do |n| %>
        <li><%= n %></li>
        <% end %>
    </ul>
<% end %>